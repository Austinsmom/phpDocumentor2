{
    "chmod": "0755",
    "directories": [
        "src",
        "vendor/twig/twig/lib/Twig/Node/Expression/Test"
    ],
    "directories-bin": ["data"],
    "files": [
        "vendor/autoload.php",
        "VERSION"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": [
                "Test",
                "Tests",
                "test",
                "tests",
                "features",
                "examples"
            ],
            "in": [
                "vendor/composer",
                "vendor/container-interop/container-interop/src",
                "vendor/erusev/parsedown",
                "vendor/league",
                "vendor/nikic/php-parser/lib",
                "vendor/nikic/php-parser/grammar",
                "vendor/php-di/invoker/src",
                "vendor/php-di/php-di/src",
                "vendor/phpdocumentor/fileset/src",
                "vendor/phpdocumentor/flyfinder/src",
                "vendor/phpdocumentor/graphviz/src",
                "vendor/phpdocumentor/reflection-common/src",
                "vendor/phpdocumentor/reflection/src",
                "vendor/phpdocumentor/reflection-docblock/src",
                "vendor/phpdocumentor/type-resolver/src",
                "vendor/psr/log/Psr",
                "vendor/symfony",
                "vendor/twig/twig/lib",
                "vendor/tedivm/stash",
                "vendor/webmozart/assert/src"
            ]
        }
    ],
    "git-version": "package_version",
    "main": "bin/phpdoc",
    "output": "phpDocumentor.phar",
    "stub": true
}
